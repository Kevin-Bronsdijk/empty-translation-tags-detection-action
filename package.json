{
  "name": "empty-translation-tags-detection-action",
  "version": "1.0.0",
  "description": "GitHub Action which detects empty translation tags",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "coverage": "jest --coverage=true",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "buildnccroot": "ncc build src/index.js -o dist --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevin-manata/lempty-translation-tags-detection-action.git"
  },
  "keywords": [],
  "author": "Manatal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kevin-manatal/empty-translation-tags-detection-action/issues"
  },
  "homepage": "https://github.com/kevin-manatal/empty-translation-tags-detection-action#readme",
  "dependencies": {
    "@actions/core": "^1.2.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.2.5",
    "jest-each": "^26.6.2",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.9.5"
  }
}
